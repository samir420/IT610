;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	samir-VirtualBox. root.localhost. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	samir-VirtualBox.linux.rocks.
@	IN	PTR	linux.rocks.
samir-VirtualBox	IN	A	192.168.1.6
sec			IN	A	192.168.1.7
client			IN	A	192.168.1.8
6	IN	PTR	samir-VirtualBox.linux.rocks.
7	IN	PTR	sec.linux.rocks.
8	IN	PTR	client.linux.rocks.
